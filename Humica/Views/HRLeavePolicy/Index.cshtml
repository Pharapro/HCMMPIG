@model Humica.Attendance.IClsLeaveType

@Html.DevExpress().PageControl(settings =>
{
    settings.Name = "pcScrollingMaterial1";
    settings.Width = Unit.Percentage(100);

    settings.EnableTabScrolling = true;
    settings.TabAlign = TabAlign.Justify;

    settings.Styles.Tab.CssClass = "scrolling_Tab";
    settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Leave Deduct Condition")).SetContent(() =>
    {
        Html.RenderPartial("GridItemLeave", Model);
    });
    settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Leave Pro-rate ")).SetContent(() =>
    {
        Html.RenderPartial("GridviewProrate", Model);
    });
    settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Leave Hours Policy")).SetContent(() =>
{
    Html.RenderPartial("GridItemLHour", Model);
});
}).GetHtml()