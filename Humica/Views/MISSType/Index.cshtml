@model Humica.Logic.Mission.ClsMISSType

@Html.DevExpress().PageControl(


        settings =>
        {
            settings.Name = "pcScrollingMaterial1";
            settings.Width = Unit.Percentage(100);

            settings.EnableTabScrolling = true;
            settings.TabAlign = TabAlign.Justify;

            settings.Styles.Tab.CssClass = "scrolling_Tab";
            settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Mission Type")).SetContent(() =>
            {
                @Html.RenderPartial("GridMMissType", Model.ListMType);

            });
        
            settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Travel By")).SetContent(() =>
            {
                @Html.RenderPartial("GridHRMTravelBy", Model.ListTravelBy);
            });
          
            settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Oil Rating")).SetContent(() =>
            {
                @Html.RenderPartial("GridHRMOilRating", Model.ListMissOilRating);

            });

            settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Mission Item")).SetContent(() =>
            {
                @Html.RenderPartial("GridMissItem", Model.ListItem);

            });
           
            settings.TabPages.Add(Humica.EF.Models.SY.SYSettings.getLabel("Claim Type")).SetContent(() =>
{
    @Html.RenderPartial("GridHRMClaimType", Model.ListMClaimType);
});


        }).GetHtml()