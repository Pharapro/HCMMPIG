  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select EFS.ReqFromDpt,EFS.EmpCode,EFS.EmpName,HL.SecDescription as level,dbo.getDateKH(EFS.ToDate) as ToDate,SP.Salary,
sp.OthAllName,dbo.getDateKH(sp.StartDate) as StartDate,HP.SecDescription as Post,HD.SecDescription as Dept,HRCompany.SecDescription as Company,sp.LevelCode,
(select Age from HR_STAFF_VIEW where HR_STAFF_VIEW.EmpCode=EFS.EmpCode) as Age,
(SELECT Status  FROM ExDocApproval ExD WHERE ExD.DocumentNo = EFS.DocNo  AND ExD.ApproveLevel = '1'  ) AS status_1,
(select Description from HRDepartment where HRDepartment.Code=EFS.ReqToDpt) as ReqToDpts,
(select Description from HRCompany where HRCompany.Company=EFS.CompanyCode) as CompanyCode,
(select Description from HRSection where HRSection.Code=EFS.Section) as Sections,
(select Description from HRPosition where HRPosition.Code=EFS.NewPosition) as NewPosition,
(SELECT TOP 1 Images FROM HRCompany WHERE SP.CompanyCode=HRCompany.GroupCompany)as Logo,
(SELECT 
    (YEAR(EFS.ToDate) - YEAR(EFS.FromDate)) * 12 
    + (MONTH(EFS.ToDate) - MONTH(EFS.FromDate)) )AS NumberOfMonths,
    (SELECT Status  FROM ExDocApproval ExD  WHERE ExD.DocumentNo = EFS.DocNo  AND ExD.ApproveLevel = '2'  ) AS status_2,
    (SELECT Status   FROM ExDocApproval ExD   WHERE ExD.DocumentNo = EFS.DocNo  AND ExD.ApproveLevel = '3' ) AS status_3,
    (select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=EFS.DocNo  and ExD.ApproveLevel='1') AS sign1,
    (select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=EFS.DocNo  and ExD.ApproveLevel='2') AS sign2,
    (select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=EFS.DocNo  and ExD.ApproveLevel='3') AS sign3,
    (SELECT Approver  FROM ExDocApproval ExD WHERE ExD.DocumentNo= EFS.DocNo  AND ExD.ApproveLevel = '1'  ) AS Approver1,
    (SELECT Approver  FROM ExDocApproval ExD  WHERE ExD.DocumentNo = EFS.DocNo AND ExD.ApproveLevel = '2'  ) AS Approver2,
    (SELECT Approver   FROM ExDocApproval ExD   WHERE ExD.DocumentNo =EFS.DocNo  AND ExD.ApproveLevel = '3' ) AS Approver3,dbo.getDateKH(EFS.FromDate) as FromDate,EFS.CareerCode,EFS.Reason,
	EFS.Level,EFS.NewPosition,EFS.NewSalary,EFS.Increase
from EFRequestTransferStaff EFS
inner join HRStaffProfile SP on SP.EmpCode=EFS.EmpCode
left join HRPosition Hp on HP.Code=Sp.JobCode
left join HRDepartment HD on HD.Code=EFS.ReqToDpt
left join HRLevel HL on HL.Code=sp.LevelCode
left join HRCompany on HRCompany.Company=SP.CompanyCode
where EFS.ID=@ID</value>
