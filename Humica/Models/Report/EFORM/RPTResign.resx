  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select  HR.EmpCode,dbo.getDateKH(HR.ResigneDate) as ResigneDate ,HR.Reason,SP.OthAllName,HR.Department,HR.Section,HR.Position,dbo.getDateKH(SP.Startdate) as Startdate,HP.SecDescription as DEPT,HP.SecDescription as Post,
 HS.SecDescription as SECT,Sp.Signature,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='1' ) status_1,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='2' ) status_2,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='3') status_3,
	(select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='1') sign1,
	(SELECT TOP 1 Images FROM HRCompany WHERE SP.CompanyCode=HRCompany.GroupCompany)as Logo,
	
	(select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='2') sign2,
	(select Signature from HRStaffProfile SHP inner join  ExDocApproval ExD on ExD.Approver= SHP.EmpCode and ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='3') sign3,
	dbo.getDateKH((select ApprovedDate from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='1' )) AppDtae1,
	dbo.getDateKH((select ApprovedDate from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='2' )) AppDtae2,
	dbo.getDateKH((select ApprovedDate from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='3' )) AppDtae3,
	(select ApprovedName from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='1' ) AS Approver1,
	(select ApprovedName from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='2' ) as Approver2,
	(select ApprovedName from ExDocApproval ExD where ExD.DocumentNo=HR.DocumentNo and ExD.DocumentType='REQRESIGN' and ExD.ApproveLevel='3' ) as Approver3

from HREmpResign HR
inner JOIN HRStaffProfile SP on SP.EmpCode=HR.EmpCode
left join HRPosition HP on HP.Code=SP.JobCode
left join HRSection HS on HS.Code=SP.SECT
left join HRDepartment HD on HD.Code=SP.DEPT


where   HR.ID=@ID</value>
