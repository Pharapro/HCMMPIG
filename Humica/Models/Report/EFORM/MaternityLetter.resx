  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select HL.EmpCode,SP.OthAllName,HL.LeaveCode,Hl.FromDate,HL.ToDate,HC.SecDescription as Company,HL.RequestDate,
HD.SecDescription as Dept,HP.SecDescription as Post,HS.SecDescription as Sect,HL.NoDay,SP.Signature,HL.Reason,hc.Images,
HL.Reason,SP.Sex,Sp.StartDate,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='1' and ExD.Approver=SP.OTFirstLine) status_1,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='2' and ExD.Approver=SP.OTSecondLine) status_2,
	(select Status from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='3' and ExD.Approver=SP.OTthirdLine) status_3,
	(select Signature from HRStaffProfile where HRStaffProfile.EmpCode=SP.FirstLine  ) sign1,
	(select Signature from HRStaffProfile where HRStaffProfile.EmpCode=SP.SecondLine  ) sign2,
	(select AllName from HRStaffProfile where HRStaffProfile.EmpCode=SP.FirstLine  ) Approver1,
	(select AllName from HRStaffProfile where HRStaffProfile.EmpCode=SP.SecondLine  ) Approver2,
	(select ApproveDate from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='1' and ExD.Approver=SP.OTFirstLine) ApproveDate_1,
	(select ApproveDate from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='2' and ExD.Approver=SP.OTSecondLine) ApproveDate_2,
	(select ApproveDate from ExDocApproval ExD where ExD.DocumentNo=HL.Increment and ExD.DocumentType='LR' and ExD.ApproveLevel='3' and ExD.Approver=SP.OTthirdLine) ApproveDate_3

from HREmpLeave HL
inner join HRStaffProfile SP on HL.EmpCode=SP.EmpCode
left join HRPosition HP on HP.Code=SP.JobCode
left join HRDepartment HD on HD.Code=SP.DEPT
left join HRCompany HC on SP.CompanyCode=HC.Company
left join HRSection HS on HS.Code=SP.SECT

where  HL.TranNo=@InCreament</value>
  </data>
