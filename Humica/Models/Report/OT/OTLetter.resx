  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select OT.OTRNo OTNo,OT.EmpCode,OT.AllName,OT.OTStartTime,OT.OTEndTime,OT.Hours,OT.Department,sp.Sex,post.Description as Position,
     OT.Reason,sp.OthAllName nameKhmer,HRCompany.Images,SP.Signature, OT.Position AS PositionEMP,
	(SELECT TOP 1 StampRejectImage FROM SYHRSetting)RejectImage,
    dbo.getDateKH(OT.CreatedOn) AS CreatedOn,
	dbo.getDateKH(GETDATE()) DateNow,
    (SELECT TOP 1 Sex from HRStaffProfile SP WHERE SP.EmpCode = OT.EmpCode ) SEXEMP,
    (CASE WHEN (SELECT COUNT(EX.Approver) FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel in(@APROLEVEL+1))&gt;0THEN 1 ELSE 0 END)N_A,
	(SELECT TOP 1 EX.Status FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(1,2) THEN 1 ELSE 0 END))Status1,
	(SELECT TOP 1 EX.Status FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(2) THEN 2 ELSE 0 END))Status2,

	dbo.getDateKH((SELECT TOP 1 EX.ApprovedDate FROM ExDocApproval EX WHERE EX.DocumentNo='REQ_OT' AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(1,2) THEN 1 ELSE 0 END)))ApprovedDate1,
	dbo.getDateKH((SELECT TOP 1 EX.ApprovedDate FROM ExDocApproval EX WHERE EX.DocumentNo='REQ_OT' AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(2) THEN 2 ELSE 0 END)))ApprovedDate2,
	
	(SELECT TOP 1 M.OthAllName FROM HRStaffProfile M WHERE M.EmpCode IN(SELECT TOP 1 EX.Approver FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(1,2) THEN 1 ELSE 0 END)))ApproverName1,
	(SELECT TOP 1 M.OthAllName FROM HRStaffProfile M WHERE M.EmpCode IN(SELECT TOP 1 EX.Approver FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(2) THEN 2 ELSE 0 END)))ApproverName2,

	(SELECT TOP 1 M.Signature FROM HRStaffProfile M WHERE M.EmpCode IN(SELECT TOP 1 EX.Approver FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(1,2) THEN 1 ELSE 0 END)))sign1,
	(SELECT TOP 1 M.Signature FROM HRStaffProfile M WHERE M.EmpCode IN(SELECT TOP 1 EX.Approver FROM ExDocApproval EX WHERE EX.DocumentNo=OT.OTRNo AND EX.DocumentType='REQ_OT' AND Ex.ApproveLevel IN(CASE WHEN @APROLEVEL in(2) THEN 2 ELSE 0 END)))sign2,
	--CONVERT(VARCHAR(10), OT.OTStartTime, 120) as startDate,
	--CONVERT(VARCHAR(10), OT.OTEndTime, 120) as endDate,
	FORMAT(OT.OTStartTime, 'hh:mm tt') AS startTime,
	FORMAT(OT.OTEndTime, 'hh:mm tt') AS endTime,
	CONVERT(VARCHAR(4), YEAR(OT.CreatedOn), 120) AS YearCreated
from HRRequestOT OT
inner JOIN HRStaffProfile SP on SP.EmpCode=OT.CreatedBy
left join HRCompany on HRCompany.Company=SP.CompanyCode
left join HRPosition post on sp.JobCode=post.Code
where OT.OTRNo= @OTRNo
and  OT.EmpCode=@EmpCode



</value>
